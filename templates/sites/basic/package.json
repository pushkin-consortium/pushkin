{
  "name": "@pushkin/template-site-basic",
  "version": "0.2.4",
  "description": "A template for building a bare-bones Pushkin site. No authentication, no forum, no dashboard, but everything else you need.",
  "keywords": [
    "pushkin",
    "template"
  ],
  "author": "L3",
  "license": "MIT",
  "files": [
    "build/*"
  ],
  "scripts": {
    "test": "jest",
    "build": "mkdir -p build && cd src && zip -FS -r -q ../build/template.zip .",
    "postinstall": "unzip build/template.zip -d $INIT_CWD"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "jest": "^29.7.0"
  }
}
