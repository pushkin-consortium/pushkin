{
    "name": "pushkin-api",
    "version": "1.6.2",
    "description": "The core Pushkin API module designed for interfacing between Pushkin clients and workers through RabbitMQ",
    "main": "build/index.js",
    "jest": {
        "transform": {
            "^.+\\.[t|j]sx?$": "babel-jest"
        },
        "transformIgnorePatterns": [
            "/node_modules/(?!(axios)/)",
            "/node_modules/(?!(core-js)/)",
            "\\.pnp\\.[^\\/]+$"
        ]
    },
    "files": [
        "build/*"
    ],
    "scripts": {
        "test": "jest",
        "build": "babel src -d build",
        "prepack": "cp ../../LICENSE ."
    },
    "keywords": [
        "pushkin",
        "api"
    ],
    "author": "L3",
    "license": "MIT",
    "homepage": "https://pushkin-consortium.github.io/pushkin",
    "repository": {
        "type": "git",
        "url": "https://github.com/pushkin-consortium/pushkin.git",
        "directory": "packages/pushkin-api"
    },
    "bugs": {
        "url": "https://github.com/pushkin-consortium/pushkin/issues"
    },
    "devDependencies": {
        "@babel/cli": "^7.22.10",
        "@babel/core": "^7.22.10",
        "@babel/plugin-transform-async-to-generator": "^7.22.5",
        "@babel/preset-env": "^7.22.10",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^29.6.3",
        "eslint": "^8.47.0",
        "supertest": "^4.0.2"
    },
    "dependencies": {
        "amqplib": "^0.10.3",
        "cookie-session": "^2.0.0-beta.3",
        "cors": "^2.8.5",
        "express": "^4.19.2",
        "regenerator-runtime": "^0.14.0",
        "uuid": "^9.0.0"
    }
}
